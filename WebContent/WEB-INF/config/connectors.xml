    <app key="svn">
        <dtype key="jira_user">
            <dbpath>svn/jira_user/</dbpath> 
        </dtype>

        <dtype key="revisions">
            <dbpath>svn/revisions/</dbpath> 
        </dtype>
    </app>  
    
    <app key="sonar">
        <dtype key="snapshot"> 
            <dbpath>sonar/snapshots/</dbpath>
        </dtype>           
    </app> 
      
    <app key="test">
        <dtype key="dtest"> 
            <dbpath>test/dtest/</dbpath>
        </dtype>           
    </app>       
             
  
    <app key="jira">
        <dtype key="user"> 
            <dbpath>jira/users/</dbpath> 
        </dtype>           
        <dtype key="issue"> 
            <dbpath>jira/issues/</dbpath> 
        </dtype>  
        <dtype key="project"> 
            <dbpath>jira/projects/</dbpath> 
        </dtype>   
    </app>
         

</connectors>
         

